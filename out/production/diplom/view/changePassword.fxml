<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="216.0" prefWidth="478.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ChangePasswordController">
    <children>
        <HBox layoutX="26.0" layoutY="51.0" prefHeight="100.0" prefWidth="427.0">
            <children>
                <VBox prefHeight="130.0" prefWidth="219.0" spacing="20.0">
                    <children>
                        <Label text="Введите новый пароль" />
                  <Label text="Повторите ввод" />
                    </children>
                    <padding>
                        <Insets bottom="1.0" left="10.0" right="1.0" top="8.0" />
                    </padding>
                </VBox>
                <VBox prefHeight="340.0" prefWidth="261.0" spacing="10.0">
               <children>
                  <PasswordField fx:id="newPasswordField" />
                  <PasswordField fx:id="newPasswordAgainField" />
               </children>
                </VBox>
            </children>
        </HBox>
        <Label layoutX="169.0" layoutY="14.0" text="Изменение пароля">
            <font>
                <Font name="System Bold" size="13.0" />
            </font>
        </Label>
        <Button fx:id="savePasswordBut" layoutX="35.0" layoutY="151.0" mnemonicParsing="false" onAction="#saveNewPassword" text="Сохранить" />
        <Button fx:id="cancelButton" layoutX="359.0" layoutY="151.0" mnemonicParsing="false" onAction="#closePasswordCh" prefHeight="25.0" prefWidth="94.0" text="Отмена" />
    </children>
</AnchorPane>
