<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="542.0" prefWidth="478.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.RegisterController">
    <children>
        <HBox layoutX="26.0" layoutY="51.0" prefHeight="100.0" prefWidth="427.0">
            <children>
                <VBox prefHeight="360.0" prefWidth="168.0" spacing="20.0">
                    <children>
                        <Label text="Фамилия" />
                        <Label text="Имя" />
                        <Label text="Отчество" />
                        <Label text="ОГРН" />
                        <Label text="ИНН" />
                        <Label text="Название" />
                        <Label text="Город" />
                        <Label text="Телефон" />
                        <Label text="E-mail" />
                        <Label text="Логин" />
                        <Label text="Пароль" />
                    </children>
                    <padding>
                        <Insets bottom="1.0" left="10.0" right="1.0" top="8.0" />
                    </padding>
                </VBox>
                <VBox prefHeight="340.0" prefWidth="261.0" spacing="10.0">
                    <children>
                        <TextField fx:id="surnameField" promptText="Введите фамилию" />
                        <TextField fx:id="nameField" promptText="Полное имя" />
                        <TextField fx:id="patroField" />
                        <TextField fx:id="ogrnField" prefHeight="25.0" prefWidth="226.0" />
                        <TextField fx:id="innField" />
                        <TextField fx:id="firmTitleField" promptText="Название фирмы" />
                        <TextField fx:id="cityField" promptText="Город нахождения фирмы" />
                        <TextField fx:id="phoneField" />
                        <TextField fx:id="emailField" />
                        <TextField fx:id="loginField" />
                        <PasswordField fx:id="passwordField" />
                    </children>
                </VBox>
            </children>
        </HBox>
        <Label layoutX="192.0" layoutY="21.0" text="Регистрация">
            <font>
                <Font name="System Bold" size="13.0" />
            </font>
        </Label>
        <Button fx:id="registerButton" layoutX="32.0" layoutY="484.0" mnemonicParsing="false" onAction="#registerAction" text="Зарегистрироваться" />
        <Button fx:id="cancelButton" layoutX="293.0" layoutY="484.0" mnemonicParsing="false" onAction="#closeReg" prefHeight="25.0" prefWidth="160.0" text="Отмена" />
    </children>
</AnchorPane>
